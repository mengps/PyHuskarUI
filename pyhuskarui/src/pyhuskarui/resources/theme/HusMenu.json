{
    "__style__": {
        "fontFamily": "@fontPrimaryFamily",
        "fontSize": "@fontPrimarySize",
        "colorTextDisabled": "@colorPrimaryTextDisabled",
        "colorText": "@colorTextBase",
        "colorTextActive": "@colorPrimaryTextHover",
        "colorBgDisabled": "transparent",
        "colorBg": "$alpha(@colorPrimaryBgActive, 0)",
        "colorBgHover": "$alpha(@colorPrimaryBgActive, 0.15)",
        "colorBgActive": "$alpha(@colorPrimaryBgActive, 0.25)",
        "colorChildBg": "@colorFillQuaternary",
        "colorEdge": "@colorSplit",
        "radiusMenuBg": "@radiusPrimary",
        "radiusPopupBg": "@radiusPrimary"
    }
}
