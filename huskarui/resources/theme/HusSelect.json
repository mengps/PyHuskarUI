{
    "__style__": {
        "fontFamily": "@fontPrimaryFamily",
        "fontSize": "@fontPrimarySize",
        "colorText": "@colorTextBase",
        "colorTextActive": "$onBackground(@colorTextTertiary, @colorBgBase)",
        "colorTextDisabled": "$onBackground(@colorPrimaryTextDisabled, @colorBgBase)",
        "colorIndicator": "$onBackground(@colorTextQuaternary, @colorBgBase)",
        "colorIndicatorHover": "$onBackground(@colorTextTertiary, @colorBgBase)",
        "colorIndicatorActive": "$onBackground(@colorTextPrimary, @colorBgBase)",
        "colorIndicatorDisabled": "$onBackground(@colorTextQuaternary, @colorBgBase)",
        "colorBorder": "@colorTextQuaternary",
        "colorBorderHover": "@colorPrimary",
        "colorBorderDisabled": "transparent",
        "colorBg": "@colorBgBase",
        "colorBgDisabled": "$onBackground(@colorFillPrimary, @colorBgBase)",
        "colorPopupBg": "@colorBgBase",
        "colorPopupBgDark": "@colorMain-11",
        "colorItemText": "@colorTextBase",
        "colorItemTextDisabled": "@colorPrimaryTextDisabled",
        "colorItemBg": "@colorBgBase",
        "colorItemBgHover": "$onBackground(@colorFillPrimary, @colorBgBase)",
        "colorItemBgActive": "@colorPrimaryBase-1",
        "colorItemBgDisabled": "@colorBgBase",
        "radiusBg": "@radiusPrimary",
        "radiusItemBg": "@radiusPrimarySM",
        "radiusPopupBg": "@radiusPrimary"
    }
}
